<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/editTextMostCommomValue3"
        android:layout_width="299dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_edit_text"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="446dp" />

    <TextView
        android:id="@+id/textView52"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:gravity="center"
        android:text="DESPESAS"
        android:textColor="@color/DarkBlue"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView41" />

    <EditText
        android:id="@+id/editTextSmallPriceFound"
        android:layout_width="181dp"
        android:layout_height="43dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_edit_text"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView53" />

    <EditText
        android:id="@+id/editTextMostBiggerValue"
        android:layout_width="180dp"
        android:layout_height="43dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_edit_text"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView53" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="PREÇO DO MERCADO"
        android:textColor="@color/DarkBlue"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView54" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:text="MAIOR PREÇO"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextSmallPriceFound"
        app:layout_constraintStart_toStartOf="@+id/editTextSmallPriceFound"
        app:layout_constraintTop_toBottomOf="@+id/editTextSmallPriceFound" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:text="MENOR PREÇO"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextMostBiggerValue"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/editTextMostBiggerValue"
        app:layout_constraintTop_toBottomOf="@+id/editTextMostBiggerValue" />

    <EditText
        android:id="@+id/editTextMostCommomValue"
        android:layout_width="299dp"
        android:layout_height="28dp"
        android:background="@drawable/rounded_edit_text"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="376dp" />

    <EditText
        android:id="@+id/editTextMostCommomValue2"
        android:layout_width="299dp"
        android:layout_height="29dp"
        android:background="@drawable/rounded_edit_text"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="Gastos Fixos"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="294dp" />

    <TextView
        android:id="@+id/textViewSugestionValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="center"
        android:text="Calculando preço de venda"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButtonAcceptValue"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/positive_button_border"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_baseline_check_24"
        app:tint="@color/DarkBlue" />

    <ImageButton
        android:id="@+id/imageButtonNotAcceptValue"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/negative_button_border"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_baseline_clear_24"
        app:tint="@color/DarkBlue" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="76dp"
        android:layout_height="70dp"
        android:tint="@color/MiddleBlue"
        app:srcCompat="@drawable/ic_baseline_calculate_24"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="539dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:tint="@color/DarkBlue"
        app:layout_constraintBottom_toBottomOf="@+id/editTextSmallPriceFound"
        app:layout_constraintEnd_toEndOf="@+id/editTextSmallPriceFound"
        app:layout_constraintTop_toTopOf="@+id/editTextSmallPriceFound"
        app:layout_constraintVertical_bias="0.571"
        app:srcCompat="@drawable/ic_baseline_chevron_right_24" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="46dp"
        android:layout_height="50dp"
        android:tint="@color/DarkBlue"
        app:layout_constraintBottom_toBottomOf="@+id/editTextMostBiggerValue"
        app:layout_constraintStart_toStartOf="@+id/editTextMostBiggerValue"
        app:layout_constraintTop_toTopOf="@+id/editTextMostBiggerValue"
        app:srcCompat="@drawable/ic_baseline_chevron_left_24" />

</androidx.constraintlayout.widget.ConstraintLayout>