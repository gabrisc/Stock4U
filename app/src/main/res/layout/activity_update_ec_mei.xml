<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".updateScreens.UpdateEcMeiActivity">

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="@drawable/positive_button_border"
        android:tint="#000000"
        app:srcCompat="@drawable/ic_baseline_check_24"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="667dp" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="@drawable/negative_button_border"
        android:onClick="deleteEconomicOperation"
        android:tint="#000000"
        app:srcCompat="@drawable/ic_baseline_clear_24"
        tools:layout_editor_absoluteX="347dp"
        tools:layout_editor_absoluteY="667dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="EDITAR PRODUTO"
        android:textColor="@color/DarkBlue"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="8dp" />

    <TextView
        android:id="@+id/textViewDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-thin"
        android:text="TextView"
        android:textColor="@color/DarkBlue"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="54dp" />

    <EditText
        android:id="@+id/editTextNameProductForEditUpdate"
        android:layout_width="363dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_edit_text"
        android:ems="10"
        android:fontFamily="sans-serif-thin"
        android:hapticFeedbackEnabled="false"
        android:hint="Nome"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="normal"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="78dp" />

    <EditText
        android:id="@+id/editTextExpenseValueforEdit"
        android:layout_width="363dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_edit_text"
        android:ems="10"
        android:fontFamily="sans-serif-thin"
        android:hapticFeedbackEnabled="false"
        android:hint="Custo/Despesas"
        android:inputType="numberDecimal"
        android:paddingLeft="10dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="normal"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="131dp" />

    <EditText
        android:id="@+id/editTextSellValueForEdit"
        android:layout_width="363dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_edit_text"
        android:ems="10"
        android:fontFamily="sans-serif-thin"
        android:hapticFeedbackEnabled="false"
        android:hint="Valor de venda"
        android:inputType="numberDecimal"
        android:paddingLeft="10dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="normal"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="185dp" />

    <Spinner
        android:id="@+id/spinnerUnidadeDeMedidaUpdate"
        android:layout_width="363dp"
        android:layout_height="33dp"
        android:background="@drawable/rounded_edit_text"
        android:spinnerMode="dialog"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="239dp" />

    <TextView
        android:id="@+id/textViewQuant"
        android:layout_width="278dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-thin"

        android:gravity="center"
        android:hapticFeedbackEnabled="false"
        android:text="Quantidade "
        android:textColor="@color/DarkBlue"
        android:textColorHint="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="normal"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="288dp" />

    <EditText
        android:id="@+id/valueOfSeekUpdate"
        android:layout_width="363dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_edit_text"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="336dp" />
</androidx.constraintlayout.widget.ConstraintLayout>